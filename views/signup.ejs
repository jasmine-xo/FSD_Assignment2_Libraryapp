<div class="container">
  <div class="row">
    
    <div class="col-md-12"> 
      
      <div class="panel">
        <div class="panel-body">
          
          
          
          <!--/stories-->
         

          <div class="col-lg-3 loginmargin">


    
           
              <form action="index.ejs" onsubmit="return validate()">
                <div class="form-group">
                    <label for="inputEmail">First Name</label>
                    <input type="text" class="form-control" id="inputfirst">
                    <label id="first" style="color:red;visibility:hidden">Invalid</label>
                </div>
                <div class="form-group">
                    <label for="inputEmail">Last Name</label>
                    <input type="text" class="form-control" id="inputlast" >
                    <label id="last" style="color:red;visibility:hidden">Invalid</label>
                </div>
                <div class="form-group">
                    <label for="inputEmail">Email</label>
                    <input type="text" class="form-control" id="inputEmail">
                    <label id="lemail" style="color:red;visibility:hidden">Invalid</label>
                </div>
                <div class="form-group">
                    <label for="inputPassword">Password</label>
                    <input type="password" class="form-control" id="inputPassword" >
                    <label id="lpwd" style="color:red;visibility:hidden">Invalid</label>
                </div>
                <div class="form-group">
                    <label for="inputPassword">Confirm Password</label>
                    <input type="password" class="form-control" id="rePassword">
                    <label id="repwd" style="color:red;visibility:hidden">Invalid</label>
                </div>
               
                <button type="submit" class="btn btn-primary">Sign in</button>
            </form>
         
