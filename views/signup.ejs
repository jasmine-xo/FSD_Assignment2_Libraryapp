	<!-- script references -->
		<script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.2/jquery.min.js"></script>
		<script src="js/bootstrap.min.js"></script>
    <script>
      function validate()
      {  
          var fir=document.getElementById("inputfirst").value;
          var las=document.getElementById("inputlast").value;
          var email=document.getElementById("inputEmail").value;
          let pwd=document.getElementById("inputPassword").value;
          let repwd=document.getElementById("rePassword").value;
          var regpwd=/^([a-zA-Z0-9]{8,20})$/;
          var regexp= /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            
          if(email=="" || pwd=="" || repwd=="" || fir=="" || las=="")
          {
              document.getElementById("first").innerHTML="Invalid";
              document.getElementById("first").style.visibility="visible";
              document.getElementById("first").style.color="red";
              document.getElementById("last").innerHTML="Invalid";
              document.getElementById("last").style.visibility="visible";
              document.getElementById("last").style.color="red";
              document.getElementById("lemail").innerHTML="Invalid";
              document.getElementById("lemail").style.visibility="visible";
              document.getElementById("lemail").style.color="red";
              document.getElementById("lpwd").innerHTML="Invalid";
              document.getElementById("lpwd").style.visibility="visible";
              document.getElementById("lpwd").style.color="red";
              document.getElementById("repwd").innerHTML="Invalid";
              document.getElementById("repwd").style.visibility="visible";
              document.getElementById("repwd").style.color="red";
          }
          if((fir.length <=2) || (fir.length > 20))
          {
          document.getElementById("first").innerHTML="Invalid";
          document.getElementById("first").style.visibility="visible";
          document.getElementById("first").style.color="red";
          return false;
          }
          if(!isNaN(fir))
          {
          document.getElementById("first").innerHTML="Invalid";
          document.getElementById("first").style.visibility="visible";
          document.getElementById("first").style.color="red";
          return false;
          }
          if((fir.length > 2) || (fir.length <= 20))
          {
          document.getElementById("first").innerHTML="valid";
          document.getElementById("first").style.visibility="visible";
          document.getElementById("first").style.color="green";
      
          }
         
          if((las.length <=2) || (las.length > 20))
          {
          document.getElementById("last").innerHTML="Invalid";
          document.getElementById("last").style.visibility="visible";
          document.getElementById("last").style.color="red";
          return false;
          }
          if(!isNaN(las))
          {
          document.getElementById("last").innerHTML="Invalid";
          document.getElementById("last").style.visibility="visible";
          document.getElementById("last").style.color="red";
          return false;
          }
          if((las.length > 2) || (las.length <= 20))
          {
          document.getElementById("last").innerHTML="valid";
          document.getElementById("last").style.visibility="visible";
          document.getElementById("last").style.color="green";
          
          }
              
              
          if(!regexp.test(email)){
              document.getElementById("lemail").innerHTML="Invalid";
              document.getElementById("lemail").style.visibility="visible";
              document.getElementById("lemail").style.color="red";
              return false;
                          
              }
          if(regexp.test(email))
          {
            document.getElementById("lemail").innerHTML="valid";
            document.getElementById("lemail").style.visibility="visible";
            document.getElementById("lemail").style.color="green";
              }

          if(!regpwd.test(pwd)) {
            document.getElementById("lpwd").innerHTML="Invalid";
            document.getElementById("lpwd").style.visibility="visible";
            document.getElementById("lpwd").style.color="red";
            return false;
          }
          if(regpwd.test(pwd)) {
            document.getElementById("lpwd").innerHTML="valid";
            document.getElementById("lpwd").style.visibility="visible";
            document.getElementById("lpwd").style.color="green";
          
          }
          if(!regpwd.test(repwd)) {
            document.getElementById("repwd").innerHTML="Invalid";
            document.getElementById("repwd").style.visibility="visible";
            document.getElementById("repwd").style.color="red";
            return false;
          }
          if(regpwd.test(repwd)) {
            document.getElementById("repwd").innerHTML="valid";
            document.getElementById("repwd").style.visibility="visible";
            document.getElementById("repwd").style.color="green";
          
          }
          if(pwd === repwd)
            {

              return true;
            }  
          
          
            }
          
  
      </script>
	</body>
